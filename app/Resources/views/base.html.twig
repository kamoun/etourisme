<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>GNU Solutions</title>
        <link rel="icon" type="image/x-icon" href="{{asset('GNUASSETS/logo/favicon.ico') }}" />
        <!-- Tell the browser to be responsive to screen width -->
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        <!-- Bootstrap 3.3.5 -->
        {% block stylesheet %}
            <link rel="stylesheet" href="{{asset('GNUASSETS/bootstrap/css/bootstrap.min.css')}}">
            <!-- Font Awesome -->
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
            <!-- Ionicons -->
            <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
            <!-- jvectormap -->
            <link rel="stylesheet" href="{{asset('plugins/jvectormap/jquery-jvectormap-1.2.2.css')}}">
            <!-- Theme style -->
            <link rel="stylesheet" href="{{asset('GNUASSETS/dist/css/AdminLTE.min.css')}}">
            <!-- AdminLTE Skins. Choose a skin from the css/skins
                 folder instead of downloading all of them to reduce the load. -->
            <link rel="stylesheet" href="{{asset('GNUASSETS/dist/css/skins/_all-skins.min.css')}}">
        {% endblock %}

        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body class="hold-transition skin-blue sidebar-mini">
        <div class="wrapper">

            <header class="main-header">

                <!-- Logo -->
                <a href="index2.html" class="logo">
                    <!-- mini logo for sidebar mini 50x50 pixels -->
                    <span class="logo-mini">GNU</span>
                    <!-- logo for regular state and mobile devices -->
                    <span class="logo-lg">GNU Solutions</span>
                </a>

                <!-- Header Navbar: style can be found in header.less -->
                <nav class="navbar navbar-static-top" role="navigation">
                    <!-- Sidebar toggle button-->
                    <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                        <span class="sr-only">Toggle navigation</span>
                    </a>
                    <!-- Navbar Right Menu -->
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">

                            <li class="dropdown user user-menu">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <img src="{{asset('GNUASSETS/dist/img/logo_gnu.png')}}" class="user-image" alt="User Image">
                                    <span class="hidden-xs">{{ app.user.username|upper}}</span>
                                </a>
                                <ul class="dropdown-menu">
                                    <!-- User image -->
                                    <li class="user-header">
                                        <img src="{{asset('GNUASSETS/dist/img/logo_gnu.png')}}" class="img-circle" alt="User Image">
                                        <p>
                                            {{ app.user.username|upper}} 

                                        </p>
                                    </li>
                                    <!-- Menu Body -->
                                    <li class="user-body">
   
                                    </li>
                                    <!-- Menu Footer-->
                                    <li class="user-footer">
                                        <div class="pull-left">
                                            <a href="#" class="btn btn-default btn-flat">Accueil</a>
                                        </div>
                                        <div class="pull-right">
                                            <a href="{{ path('fos_user_security_logout')}}" class="btn btn-default btn-flat">DÃ©connecter</a>
                                        </div>
                                    </li>
                                </ul>
                            </li>
                            <!-- Control Sidebar Toggle Button -->
                            <li>
                                <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                            </li>
                        </ul>
                    </div>

                </nav>
            </header>
            <!-- Left side column. contains the logo and sidebar -->
            <aside class="main-sidebar">
                <!-- sidebar: style can be found in sidebar.less -->
                <section class="sidebar">
                    <!-- Sidebar user panel -->
                    <div class="user-panel">
                        <div class="pull-left image">
                            <img src="{{asset('GNUASSETS/dist/img/logo_gnu.png')}}" class="img-circle" alt="User Image">
                        </div>
                        <div class="pull-left info">
                            <p>{{ app.user.username|upper}}</p>
                            <a href="#"><i class="fa fa-circle text-success"></i> En ligne</a>
                        </div>
                    </div>
                    <!-- search form -->
                    <form action="#" method="get" class="sidebar-form">
                        <div class="input-group">
                            <input type="text" name="q" class="form-control" placeholder="Recherche...">
                            <span class="input-group-btn">
                                <button type="submit" name="search" id="search-btn" class="btn btn-flat"><i class="fa fa-search"></i></button>
                            </span>
                        </div>
                    </form>
                    <!-- /.search form -->
                    <!-- sidebar menu: : style can be found in sidebar.less -->
                    {% include 'UsersBundle:UsedMenu:leftmenu.html.twig' %}
                </section>
                <!-- /.sidebar -->
            </aside>

            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper">
                <!-- Content Header (Page header) -->
                {% block content %}
                <div id="flash-messages">  
                    {% for flashMessage in app.session.flashbag.get('success') %}
                        <div class=" alert alert-success">
                            <button class="close" data-dismiss="alert">&times;</button>
                            {{ flashMessage|trans|raw|nl2br }}
                        </div>
                    {% endfor %}

                    {% for flashMessage in app.session.flashbag.get('error') %}
                        <div class="  alert alert-error">
                            <button class="close" data-dismiss="alert">&times;</button>
                            {{ flashMessage|trans|raw|nl2br }}
                        </div>
                    {% endfor %}

                    {% for flashMessage in app.session.flashbag.get('info') %}
                        <div class=" alert alert-info">
                            <button class="close" data-dismiss="alert">&times;</button>
                            {{ flashMessage|trans|raw|nl2br }}
                        </div>
                    {% endfor %}
                </div> 
              
                {% endblock %}
            </div><!-- /.content-wrapper -->

            <footer class="main-footer navbar-default navbar-fixed-bottom">
                <div class="pull-right hidden-xs">
                    <b>Version</b> 2.3.0
                </div>
                <strong>Copyright &copy; 2016-2017 <a href="http://www.gnusolutions.com.tn" target="_blank">Gnu Solutions</a>.</strong> All rights reserved.
            </footer>

            <!-- Control Sidebar -->
            <aside class="control-sidebar control-sidebar-dark">
              
                <!-- Tab panes -->
                <div class="tab-content">
                    <!-- Home tab content (ascenceur) -->
                    <div class="tab-pane" id="control-sidebar-home-tab">
                    </div><!-- /.tab-pane (ascenceur) -->
                </div>
            </aside><!-- /.control-sidebar -->
            <!-- Add the sidebar's background. This div must be placed
                 immediately after the control sidebar -->
            <div class="control-sidebar-bg"></div>

        </div><!-- ./wrapper -->
        {% block javascript %}
        <!-- jQuery 2.1.4 -->
        <script src="{{asset('GNUASSETS/plugins/jQuery/jQuery-2.1.4.min.js')}}"></script>
        <!-- Bootstrap 3.3.5 -->
        <script src="{{asset('GNUASSETS/bootstrap/js/bootstrap.min.js')}}"></script>
        <!-- FastClick -->
        <script src="{{asset('GNUASSETS/plugins/fastclick/fastclick.min.js')}}"></script>
        <!-- AdminLTE App -->
        <script src="{{asset('GNUASSETS/dist/js/app.min.js')}}"></script>
        <!-- Sparkline -->
        <script src="{{asset('GNUASSETS/plugins/sparkline/jquery.sparkline.min.js')}}"></script>
        <!-- jvectormap -->
        <script src="{{asset('GNUASSETS/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js')}}"></script>
        <script src="{{asset('GNUASSETS/plugins/jvectormap/jquery-jvectormap-world-mill-en.js')}}"></script>
        <!-- SlimScroll 1.3.0 -->
        <script src="{{asset('GNUASSETS/plugins/slimScroll/jquery.slimscroll.min.js')}}"></script>
        <!-- ChartJS 1.0.1 -->
        <script src="{{asset('GNUASSETS/plugins/chartjs/Chart.min.js')}}"></script>
        <!-- AdminLTE for demo purposes -->
        <script src="{{asset('GNUASSETS/dist/js/demo.js')}}"></script>
        {% endblock %}
    </body>
</html>
